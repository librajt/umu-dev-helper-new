{
  "author": "daoyu@umu.com",
  "manifest_version": 2,
  "name": "umu dev helper",
  "version": "1.9.17",
  "description": "切换开发版本,支持快捷键;日语版同声传译;自动填写问卷;modao文本可选择;隐藏位置;线下域名跳转;网格标尺; ",
  "background": {
    "scripts": [
    ]
  },
  "icons": {
    "16": "images/umu16.png",
    "48": "images/umu48.png",
    "128": "images/umu128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.umucdn.cn/*",
        "https://modao.cc/*",
        "https://www.umu.cn/",
        "http://www.umu.co/",
        "http://www.umu.com/",
        "http://www.umu.tw/",
        "http://www.umu.hk/"
      ],
      "exclude_matches": [
        "http://gitlab.umucdn.cn/*"
      ],
      "js": [
        "js/content.js"
      ],
      "css":[
        "css/umu-dev-helper.css"
      ]
    }

  ],
  "permissions": [
    "http://*.umucdn.cn/",
    "https://modao.cc/",
    "https://*.amazonaws.com/",
    "https://www.umu.cn/",
    "http://www.umu.co/",
    "http://www.umu.com/",
    "http://www.umu.tw/",
    "http://www.umu.hk/",
    "http://*.qiniucdn.com/"
  ],
  "web_accessible_resources": [
    "css/umu-dev-helper.css",
    "/images/avatar/duanjun.png",
    "/images/avatar/liudaoyu.png"
  ],
  "homepage_url": "https://github.com/daoyuly/umu-dev-helper-new"
}
